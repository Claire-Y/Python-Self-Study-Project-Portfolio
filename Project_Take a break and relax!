#Tired of doing work? Take a break and run this file! Have a relax and recharge!

import time
import webbrowser

total_break = 3
break_count = 0

print ('The program started on' + time.ctime())
while break_count < total_break:
    time.sleep(10)
    webbrowser.open('https://www.youtube.com/watch?v=09R8_2nJtjg')
    break_count +=1
